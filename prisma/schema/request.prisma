model Request {
    id                  String                @id @default(uuid())
    createdAt           DateTime              @default(now())
    updatedAt           DateTime              @updatedAt
    text String 
    statusId String
    status  Status @relation(references: [id], fields: [statusId], onDelete: Cascade)
    typeId String
    type Type @relation(references: [id], fields: [typeId], onDelete: Cascade)
}




